<div id="carouselHero" class="carousel slide" data-ride="carousel">

  <!-- Indicators -->
  <ul class="carousel-indicators">
    <li data-target="#carouselHero" data-slide-to="0" class="active"></li>
    <li data-target="#carouselHero" data-slide-to="1"></li>
    <li data-target="#carouselHero" data-slide-to="2"></li>
  </ul>
  <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img class="d-block w-100"
          src="https://images.unsplash.com/photo-1580041065738-e72023775cdc?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=1500&q=80"
          alt="First slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100"
          src="https://images.unsplash.com/photo-1614957004131-9e8f2a13123c?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=1500&q=80"
          alt="Second slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100"
          src="https://images.unsplash.com/photo-1496417263034-38ec4f0b665a?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=751&q=80"
          alt="Third slide">
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>

<div class="title-header">
  <div class="Home-Section">
    <h4>Browse By Property Type</h4>
    <div class="row">
      {{#each locationSort}}
      <div class="col-sm">
        <p>{{ this.type }}</p>
        <a href="/product/{{ this.id }}">
          <img src="{{ this.smllimg }}" class="home-pics img-thumbnail d-block w-100" alt="...">
        </a>
      </div>
      {{/each}}
    </div>
  </div>
  <hr>

  <div class="Home-Section">
    <h4>Browse By Location</h4>
    <div class="row">
      {{#each locationSort}}
      <div class="col-sm">
        <p>{{ this.location }}</p>
        <a href="/product/{{ this.id }}">
          <img src="{{ this.smllimg }}" class="home-pics img-thumbnail d-block w-100" alt="...">
        </a>
      </div>
      {{/each}}
    </div>
  </div>
  <hr>

  <div class="Home-Section">
    <h4>Hot Deals!</h4>
    <div class="row">
      {{#each locationSort }}
      <div class="col-sm">
        <h5>Vacation Deals</h5>
        <figure class="figure">
          <a href="/product/{{ this.id }}">
            <img src="{{this.smllimg}}" class="home-pics figure-img img-fluid rounded" alt="...">
          </a>
          <figcaption class="figure-caption">
            {{ this.details }}<br>
            Star Rating: {{ this.starRating }}
          </figcaption>
        </figure>
      </div>
      {{/each}}
    </div>
  </div>